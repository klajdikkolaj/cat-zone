datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  output          = "../src/common/generated/client"
  previewFeatures = ["fullTextSearch", "postgresqlExtensions"]
  }

model Photo {
  id          Int      @id @default(autoincrement())
  publishedAt DateTime
  url         String
  tags        String[]
}
